!config:
realm: Example realm
compressionEnabled: true

repositoryMapping: !gitlabMapping
  path: /var/git/repositories/
  template:
    branch: master
    renameDetection: true

acl: {}

shared:
  # Web server settings.
  - !web
    baseUrl: http://git-as-svn.local/
    listen:
    - !http
      host: localhost
      port: 8123
  # Git LFS server
  - !lfs
    path: lfs
    # token: secret
  # GitLab server
  - !gitlab
    url: http://localhost/
    token: yKZBNG2JzyzvUBoevdM4

# Simple in-memory user database
userDB: !gitlabUsers {}
