<?xml version="1.0" encoding="utf-8" ?>
<fop version="1.0">
	<font-base>src/fonts</font-base>
	<source-resolution>72</source-resolution>
	<target-resolution>72</target-resolution>
	<renderers>
		<renderer mime="application/pdf">
			<fonts>
				<font kerning="yes" embed-url="freefont/FreeMonoBoldOblique.ttf">
					<font-triplet name="mono" style="italic" weight="bold" />
				</font>
				<font kerning="yes" embed-url="freefont/FreeMonoBold.ttf">
					<font-triplet name="mono" style="normal" weight="bold" />
				</font>
				<font kerning="yes" embed-url="freefont/FreeMonoOblique.ttf">
					<font-triplet name="mono" style="italic" weight="normal" />
				</font>
				<font kerning="yes" embed-url="freefont/FreeMono.ttf">
					<font-triplet name="mono" style="normal" weight="normal" />
				</font>
				<font kerning="yes" embed-url="freefont/FreeSansBoldOblique.ttf">
					<font-triplet name="sans-serif" style="italic" weight="bold" />
				</font>
				<font kerning="yes" embed-url="freefont/FreeSansBold.ttf">
					<font-triplet name="sans-serif" style="normal" weight="bold" />
				</font>
				<font kerning="yes" embed-url="freefont/FreeSansOblique.ttf">
					<font-triplet name="sans-serif" style="italic" weight="normal" />
				</font>
				<font kerning="yes" embed-url="freefont/FreeSans.ttf">
					<font-triplet name="sans-serif" style="normal" weight="normal" />
				</font>
				<font kerning="yes" embed-url="freefont/FreeSerifBoldItalic.ttf">
					<font-triplet name="serif" style="italic" weight="bold" />
				</font>
				<font kerning="yes" embed-url="freefont/FreeSerifBold.ttf">
					<font-triplet name="serif" style="normal" weight="bold" />
				</font>
				<font kerning="yes" embed-url="freefont/FreeSerifItalic.ttf">
					<font-triplet name="serif" style="italic" weight="normal" />
				</font>
				<font kerning="yes" embed-url="freefont/FreeSerif.ttf">
					<font-triplet name="serif" style="normal" weight="normal" />
				</font>
			</fonts>
		</renderer>
	</renderers>
</fop>
